#include<iostream>
int main()
{
	int sayi;
	int i;
	int sayi1=0;
	int sayi2=0;
	
for(i=0; i<=100; i++)
   {
   	sayi1=(i*i)+sayi1;
   	
	}	
	for(i=0; i<=100; i++)
	{
	sayi2=sayi2+i;
	}
	sayi2=sayi2*sayi2;
	printf("Sayi1 %d\n",sayi1);
	printf("Sayi2 %d\n",sayi2);
	printf("iki deger arasindaki fark %d\n",sayi2-sayi1);
}
